    "def is_prime(I):\n",
    "    if I % 2==0 : return False\n",
    "    for i in range(3, int (I**0.5)+ 1,2):\n",
    "        if I % i ==0: return False\n",
    "    return True\n",
    "\n",
    "n = int(2**64) +1\n",
    "display (n)\n",
    "%time is_prime(100109100129162907)\n",
    "\n"
